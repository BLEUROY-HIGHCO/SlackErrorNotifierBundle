{
    "name": "highco/slack-error-notifier-bundle",
    "type": "symfony-bundle",
    "description": "This bundle sends a markdown formated message to slack when an error occurs.",
    "keywords": [
        "higco",
        "error",
        "notifier",
        "markdown",
        "slack",
        "bundle"
    ],
    "homepage": "http://github.com/BLEUROY-HIGHCO/SlackErrorNotifierBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Highco Team",
            "homepage": "https://www.highco.com"
        },
        {
            "name": "Contributors",
            "homepage": "http://github.com/BLEUROY-HIGHCO/SlackErrorNotifierBundle/contributors"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/BLEUROY-HIGHCO/slack.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "symfony/console": "~2.3|~3.0",
        "monolog/monolog": "~1.9",
        "cleentfaar/slack": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/phpunit-bridge": "~3.3"
    },
    "autoload": {
        "psr-4": {
            "Highco\\SlackErrorNotifierBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
